    //Game Loop
    while (window.isOpen()) {

        sf::Event event;
        while (window.pollEvent(event))
        {
            if (event.type == sf::Event::Closed)
                window.close();
        }

        window.clear(); //Clear Window

        //PLAYER MOVEMENT
        if (sf::Keyboard::isKeyPressed(sf::Keyboard::Up)) //Move Up
            player->moveUp();

        else if(sf::Keyboard::isKeyPressed(sf::Keyboard::Down)) //Move Down
            player->moveDown();

        else if(sf::Keyboard::isKeyPressed(sf::Keyboard::Right)) //Move Right
            player->moveRight();

        else if(sf::Keyboard::isKeyPressed(sf::Keyboard::Left)) //Move Left
            player->moveLeft();



        //SPRINT
/*         if(sf::Keyboard::isKeyPressed(sf::Keyboard::LShift))
            player.setSpeed(0.35, sf::milliseconds(50));
        else
            player.setSpeed(0.20, sf::milliseconds(80));
 */
        //Draw Sprites

        for(auto& element : map){
         
            
            window.draw(element.second->getSprite());
            
        }
        //window.draw(player.getSprite()); 


        

        window.display(); //Display Window
    }